{
    "suggest.floatConfig": {
        "border": true
    },
    "diagnostic.floatConfig": {
        "border": true
    },
    "signature.floatConfig": {
        "border": true
    },
    "hover.floatConfig": {
        "border": true
    },
    "suggest.noselect": false,
    "solargraph.commandPath": "~/.local/share/mise/installs/ruby/3.3.9/bin/solargraph",
    "go.goplsOptions": {
        "buildFlags": ["-tags=integration,e2e,wireinject"],
        "local": "github.com/bitrise-io,github.com/bitrise-steplib"
    },
    "go.goplsEnv":{
        "GOFLAGS": "-tags=integration,e2e,wireinject"
    },
    "tslint.autoFixOnSave": true,
    "eslint.autoFixOnSave": true,
    "diagnostic.virtualText": true,
    "diagnostic.virtualTextCurrentLineOnly": false,
    "languageserver": {
        "golangci-lint-languageserver": {
            "command": "golangci-lint-langserver",
            "filetypes": ["go"],
            "initializationOptions": {
                "command": ["golangci-lint", "run", "--skip-dirs", ".*vendor.*", "--output.json.path", "stdout", "--show-stats=false", "--issues-exit-code=1"]
            }
        },
        "sorbet": {
            "command": "srb",
            "args": ["tc", "--lsp", "--enable-all-experimental-lsp-features"],
            "filetypes": ["rubyyyy"],
            "rootPatterns": ["sorbet/config"],
            "initializationOptions": {},
            "settings": {}
        },
        "ruby-lsp": {
            "command": "ruby-lsp",
            "filetypes": ["rubyyyy"],
            "rootPatterns": ["Gemfile"],
            "initializationOptions": {
                "enabledFeatures": [
                    "codeActions",
                    "diagnostics",
                    "documentHighlights",
                    "documentLink",
                    "documentSymbols",
                    "foldingRanges",
                    "formatting",
                    "hover",
                    "inlayHint",
                    "onTypeFormatting",
                    "selectionRanges"
                ]
            },
            "settings": {}
        }
    },
    "snippets.ultisnips.pythonPrompt": false
}
